{
  "name": "expo-payload",
  "license": "0BSD",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "NODE_OPTIONS=\"--max-old-space-size=8192\" expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest __tests__ --watchAll",
    "test:ci": "jest __tests__",
    "lint": "expo lint",
    "lint:fix": "expo lint --fix",
    "prettier:write": "prettier --write .",
    "database:reset": "bun run scripts/reset.ts",
    "generate:types": "bunx --bun payload generate:types --disable-transpile",
    "migrate:auth": "bunx @better-auth/cli migrate --config lib/auth.ts --yes"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@better-auth/expo": "1.3.7",
    "@payloadcms/db-postgres": "^3.54.0",
    "@payloadcms/email-resend": "^3.54.0",
    "@payloadcms/storage-uploadthing": "^3.54.0",
    "@react-navigation/native": "^7.0.0",
    "@t3-oss/env-core": "^0.13.8",
    "better-auth": "1.3.7",
    "expo": "^54.0.0-preview.16",
    "expo-camera": "~17.0.6",
    "expo-constants": "~18.0.7",
    "expo-dev-client": "~6.0.10",
    "expo-haptics": "~15.0.6",
    "expo-image": "~3.0.7",
    "expo-linking": "~8.0.7",
    "expo-router": "~6.0.0-preview.16",
    "expo-secure-store": "~15.0.6",
    "expo-splash-screen": "~31.0.8",
    "expo-status-bar": "~3.0.7",
    "expo-symbols": "~1.0.6",
    "expo-system-ui": "~6.0.6",
    "expo-web-browser": "~15.0.6",
    "payload": "^3.54.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.1",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "^0.21.0",
    "react-server-dom-webpack": "^19.1.1",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@reactioncommerce/babel-remove-es-create-require": "^1.0.0",
    "@testing-library/react-native": "^13.3.3",
    "@types/bun": "^1.1.12",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.1.10",
    "babel-plugin-transform-import-meta": "^2.2.1",
    "eslint": "^9.0.0",
    "eslint-config-expo": "~10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "~29.7.0",
    "jest-expo": "~54.0.8",
    "prettier": "^3.6.2",
    "react-test-renderer": "19.1.0",
    "typescript": "~5.9.2"
  },
  "private": true,
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "@payloadcms/db-postgres",
          "@payloadcms/email-resend",
          "@payloadcms/storage-uploadthing",
          "payload"
        ]
      }
    }
  },
  "packageManager": "bun@1.2.21"
}
